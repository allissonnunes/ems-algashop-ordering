{
  "request": {
    "method": "POST",
    "url": "/api/delivery-cost",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.originZipCode",
          "equalTo": "12345"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.destinationZipCode",
          "equalTo": "12345"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "deliveryCost": "35.00",
      "estimatedDaysToDeliver": "7"
    }
  }
}